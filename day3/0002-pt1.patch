+	return 0;
+}
+
+void rearange(std::string &s)
+{
+	std::sort(s.begin(), s.end());
+	auto end = std::unique(s.begin(), s.end());
+	s.erase(end-s.begin());
+}
+
+char findCommon(const std::string &lhs, const std::string &rhs)
+{
+	for(char var : lhs) {
+		if (rhs.find(var) != std::string::npos) {
+			return var;
+		}
+	}
+	return ' ';
+}
+
+int getWeight(char c)
+{
+	if (c >= 'a' && c <= 'z') {
+		return c - 'a' + 1;
+	} else if (c >= 'A' && c <= 'Z') {
+		return c - 'A' + 27;
 	}
 	return 0;
 }
