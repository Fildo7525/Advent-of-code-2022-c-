@@ -1,8 +1,79 @@
 #include "day2.h"
+#include <fstream>
+#include <string>
+#include <vector>
+
+enum class rps {
+	ROCK,
+	PAPER,
+	SCISORS,
+};
+
+int deciferWin(rps other, rps me);
+rps getHis(char c);
+rps getMe(char c);
+int getPointsFromSign(rps sin);
 
 int main(int argc, char *argv[])
 {
-	fstream input("../input", std::ios::in);	std::cout << "Hello World!\n";
+	std::fstream input("../input", std::ios::in);
+	std::string line;
+	int totalScore = 0;
+	while (std::getline(input, line)) {
+		int win = deciferWin(getHis(line[0]), getMe(line[2]));
+		totalScore += getPointsFromSign(getMe(line[2])) + win;
+	}
+	std::cout << "Score: " << totalScore << std::endl;
+	return 0;
+}
+
+int deciferWin(rps other, rps me)
+{
+	if (other == rps::ROCK && me == rps::PAPER) {
+		return 6;
+	} else if (other == rps::PAPER && me == rps::SCISORS) {
+		return 6;
+	} else if (other == rps::SCISORS && me == rps::ROCK) {
+		return 6;
+	} else if (other == me) {
+		return 3;
+	}
 	return 0;
 }
 
+rps getHis(char c)
+{
+	switch (c) {
+		case 'A':
+			return rps::ROCK;
+		case 'B':
+			return rps::PAPER;
+		case 'C':
+			return rps::SCISORS;
+	}
+}
+
+rps getMe(char c)
+{
+	switch (c) {
+		case 'X':
+			return rps::ROCK;
+		case 'Y':
+			return rps::PAPER;
+		case 'Z':
+			return rps::SCISORS;
+	}
+}
+
+int getPointsFromSign(rps sin)
+{
+	switch (sin) {
+		case rps::ROCK:
+			return 1;
+		case rps::PAPER:
+			return 2;
+		case rps::SCISORS:
+			return 3;
+	}
+}
+
